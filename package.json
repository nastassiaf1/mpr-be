{
  "description": "RESTful API with Node.js, Express, and PostgreSQL",
  "main": "app.js",
  "dependencies": {
    "body-parser": "^1.20.2",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "babel-register": "^6.26.0",
    "nodemon": "^2.0.22",
    "pg": "^8.11.0",
    "sequelize": "^6.32.0",
    "sequelize-cli": "^6.6.1",
    "npx": "^10.2.2",
    "path": "^0.12.7"
  },
  "scripts": {
    "start": "node app.js",
    "devStart": "nodemon app.js",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:create-migration": "npx sequelize-cli migration:create --name",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo"
  },
  "type": "module"
}
